{
  "name": "intor-cache",
  "version": "1.0.0",
  "description": "A lightweight in-memory cache with TTL and size limit, used in the Intor i18n library to efficiently store translation messages.",
  "author": "Yiming Liao",
  "license": "MIT",
  "homepage": "https://github.com/yiming-liao/intor-cache#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yiming-liao/intor-cache"
  },
  "bugs": {
    "url": "https://github.com/yiming-liao/intor-cache/issues"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "translation",
    "typescript",
    "cache",
    "message-cache",
    "translator",
    "performance",
    "intor",
    "node"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "prepublishOnly": "yarn build"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "next": "^15.3.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
